<script>
//시작시 큰 로고 움직이기
$('window').ready(function () {
	$('.col90').eq(0).addClass("active fp-completely");
})

// 클릭 시 선택 유지
$('a').on("click", function () {
    $('a').removeClass("active"); 
    $(this).addClass("active"); 
});


//a태그 클릭시 스크롤로 이동하기
$("a").on("click", function (e) {
    e.preventDefault();

    const index = $("a").index(this); // 2부터 시작
   
	if(index >= 2){
		const st = 911*(index-2);
    	$("html, body").animate({ scrollTop: st }, 500);
	}
});

window.addEventListener(
		"wheel", function (e) {
			e.preventDefault();

		    const delta = e.deltaY; //휠 방향을 감지
			const point = $(window).scrollTop();
		    
			if(delta < 0){	//마우스 휠을 올렸을때
		    	$("html, body").stop(true).animate({ scrollTop: point-911 }, 500);
				}else if (delta > 0) { //마우스 휠을 내렸을때
		    	$("html, body").stop(true).animate({ scrollTop: point+911 }, 500);
				}
		}
		, { passive: false }
		);
		
		
$(window).on("scroll", function () {
	$('a').removeClass("active"); 
	$('.col90').removeClass("active fp-completely");
    const st = $(window).scrollTop();
	//console.log(st);
	if(st < 911){
	$("a").eq(2).addClass("active");
	$('.col90').eq(0).addClass("active fp-completely");
	}else if(st >= 911 && st < 1822){
	$("a").eq(3).addClass("active");
	$('.col90').eq(1).addClass("active fp-completely");
	}else if (st >= 1822 && st < 2733) {
	$("a").eq(4).addClass("active");
	$('.col90').eq(2).addClass("active fp-completely");
	}else if (st >= 2733 && st < 3644) {
	$("a").eq(5).addClass("active");
	$('.col90').eq(3).addClass("active fp-completely");
	}else if (st >= 3644) {
	$("a").eq(6).addClass("active");
	$('.col90').eq(4).addClass("active fp-completely");
	}
})
		
$(document).on("keydown", function(e) {
    const st = $(window).scrollTop();

    if (e.key === "ArrowUp" || e.keyCode === 38 || e.keyCode === 33) { // 위쪽 방향키
        $("html, body").stop(true).animate({ scrollTop: st - 911 }, 500);
        e.preventDefault();
    } else if (e.key === "ArrowDown" || e.keyCode === 40 || e.keyCode === 34) { // 아래쪽 방향키
        $("html, body").stop(true).animate({ scrollTop: st + 911 }, 500);
        e.preventDefault(); 
    }
});		
		
</script>
 
